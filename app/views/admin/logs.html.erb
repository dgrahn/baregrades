<% content_for :head do %>
	<h1>Logs</h1>
<% end %>


<table class="table table-bordered">
	<thead>
		<tr>
			<th>Date</th>
			<th>Description</th>
		</tr>
	</thead>
	<tbody>
		<% @logs.each do |log| %>
			<tr>
				<td><%= log.created_at %></td>
				<td><%= log.comments %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<% render "admin/navigation" %>